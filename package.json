{
  "name": "ts-express-decorators",
  "version": "2.0.0-2",
  "description": "Use TypeScript decorators to create your Express application and Rest API",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf test/*.js test/**/*.js test/*.js.map test/**/*.js.map test/**/**/*.js test/**/**/*.js.map src/**/*.js src/**/*.js.map src/**/**/*.js src/**/**/*.js.map",
    "prepublish": "npm run tsc:compile",
    "tslint": "tslint ./src/*.ts",
    "test": "npm run clean && npm run tsc && npm run tslint && NODE_ENV=test nyc --reporter=html --reporter=text _mocha --recursive",
    "coveralls:travis": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:compile": "rm -rf ./lib && tsc --project tsconfig.compile.json",
    "doc:serve": "concurrently \"tsc --project tsconfig.compile.json -w\" \"docsify serve ./docs\" \"cd docs/_build && nodemon --delay 2.5 -e ts --watch ../../lib build-api.js\"",
    "doc:build": "cd docs/_build && node build-api.js && cd ../..",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "semantic-release-locale": "semantic-release pre --debug=true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Romakita/ts-express-decorators.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "Decorator",
    "decorators",
    "decorator",
    "typescript",
    "express",
    "Controller",
    "Inject",
    "ioc",
    "ES2015",
    "ES6"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Romakita/ts-express-decorators/issues"
  },
  "homepage": "https://github.com/Romakita/ts-express-decorators#readme",
  "dependencies": {
    "glob": "^7.0.3",
    "reflect-metadata": "^0.1.8",
    "source-map-support": "^0.4.15",
    "ts-httpexceptions": "^2.2.1",
    "ts-log-debug": "^3.0.0",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/chai-as-promised": "0.0.31",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/mocha": "^2.2.31",
    "@types/node": "^7.0.0",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^2.2.1",
    "@types/sinon-chai": "^2.7.27",
    "@types/superagent": "^2.0.34",
    "@types/supertest": "^2.0.0",
    "@types/swagger-schema-official": "^2.0.5",
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "compression": "^1.6.1",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.1",
    "coveralls": "^2.13.1",
    "docsify-cli": "^4.1.9",
    "docsify-server-renderer": "^4.2.6",
    "ejs": "^2.5.5",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fs-extra": "^4.0.0",
    "html-minifier": "^3.5.2",
    "istanbul": "^0.4.5",
    "method-override": "^2.3.5",
    "mocha": "^3.3.0",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "nyc": "^11.0.3",
    "proxyquire": "^1.8.0",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "supertest": "^3.0.0",
    "swagger-ui-express": "^2.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typedoc": "0.5.10",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.4.0"
  },
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "release": {
    "debug": false,
    "branch": "production"
  }
}
